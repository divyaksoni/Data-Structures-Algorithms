class Solution {
  public:
    vector<string> AllPossibleStrings(string s) 
    {
        int n=s.length();
        
        vector<string>solution;
        string ans="";
        findPowerSet(0, s, ans, solution);
        sort(solution.begin(), solution.end());
        return solution;
        
    }
    
    void findPowerSet(int i, string &s, string &ans, vector<string>&solution)
    {
        //base
        if(i==s.length())
        {
            if(ans.length()!=0)
            {
                // cout<<ans<<endl;
                solution.push_back(ans);
            }
            return;
        }
        //reccurance
        
        //take the ith element
        ans.push_back(s[i]);
        findPowerSet(i+1, s, ans, solution);
        
        ans.pop_back();
        //not take the element
        findPowerSet(i+1, s, ans, solution);
    }
};
